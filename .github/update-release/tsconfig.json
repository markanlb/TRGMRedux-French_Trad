{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "rootDir": ".",
    "moduleResolution": "Node",
    "noEmit": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "newLine": "lf",
    "allowJs": true,
    "checkJs": true
  },
  "exclude": ["./node_modules"]
}